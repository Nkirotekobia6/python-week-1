ef calculate(num1,num2,operator):
    if operator == '+':
        return num1 + num2
    elif operator == '-':
        return num1 - num2
    elif operator == '*':
        return num1 * num2
    elif operator == '/':
     return num1 / num2
    else: 
       return "invalid operator"
    
    num1 = 7
    num2 = 9
    operator = "*"

    result = calculate(num1, num2, operator)

print(f"{"num1"} {"operator"} {"num2"} = {"result"}")
